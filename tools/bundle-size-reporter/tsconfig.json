{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": ["es2019"],
    "sourceMap": true,
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "skipLibCheck": true
  },
}
