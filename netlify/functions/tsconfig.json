{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "sourceMap": true,
    "isolatedModules": true,
    "types": [
      "node"
    ],
    "baseUrl": "."
  },
  "include": [
    "**/*.ts",
  ]
}
